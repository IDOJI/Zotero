issn = {01970186},
journal = {Neurochemistry International},
keywords = {Acquired channels,Ca 2+ influx,Cytotoxicity,NMDA receptor,NR3 subunit},
number = {7},
pages = {730--737},
publisher = {Elsevier Ltd},
title = {{Requirement of both NR3A and NR3B subunits for dominant negative properties on Ca 2+ mobilization mediated by acquired N-methyl-d-aspartate receptor channels into mitochondria}},
