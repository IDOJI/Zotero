doi = {10.1016/j.cmpb.2016.10.007},
file = {:C$\backslash$:/Users/SHIGURE/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gao, Hui, Tian - 2017 - Classification of CT brain images based on deep learning networks.pdf:pdf},
issn = {18727565},
journal = {Computer Methods and Programs in Biomedicine},
keywords = {3D CNN,CT brain images,Classification,Convolutional neural network,Deep learning,KAZE},
pages = {49--56},
publisher = {Elsevier Ireland Ltd},
title = {{Classification of CT brain images based on deep learning networks}},
url = {http://dx.doi.org/10.1016/j.cmpb.2016.10.007},
volume = {138},
