file = {:C$\backslash$:/Users/SHIGURE/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Boger et al. - 2012 - Measures of small-fiber neuropathy in HIV infection.pdf:pdf},
issn = {15660702},
journal = {Autonomic Neuroscience: Basic and Clinical},
keywords = {Antiretroviral,HIV,Neuropathy,QSART,UENS},
number = {1},
