@article{Teixeira2012,
abstract = {Mild cognitive impairment (MCI) can be a stage of pre-dementia. There is no consensus about pharmacological treatment for this population, so it is important to structure non-pharmacological interventions for increasing their cognitive reserve. We intended to analyze the effects of non-pharmacological interventions in the cognitive functions in older people with MC, in form of a systemic review. Data sources were the Web of Science, Biological Abstracts, Medline, Pub Med, EBSCHost, Scirus and Google Scholar. All studies were longitudinal trials, with MCI sample, aged {\textgreater} 60 years, community-dwelling, and having cognitive functions as dependent variable. Seven studies, from 91 previously selected ones, were identified according to the inclusion criteria. Six studies used cognitive intervention, improving memory and one study used physical activity as intervention, improving executive functions. The results show evidence that physical activity and cognitive exercise may improve memory and executive functions in older people with MCI. But yet, more controlled studies are needed to establish a protocol of recommendations regarding the systemization of exercise, necessary to produce benefits in the cognitive functioning in older people with MCI. {\textcopyright} 2011 Elsevier Ireland Ltd.},
author = {Teixeira, Camila Vieira Ligo and Gobbi, Lilian Teresa Bucken and Corazza, Danilla Icassatti and Stella, Florindo and Costa, Jos{\'{e}} Luiz Riani and Gobbi, Sebasti{\~{a}}o},
doi = {10.1016/j.archger.2011.02.014},
file = {:C$\backslash$:/Users/SHIGURE/Box Sync/논문/paper{\_}teixeira{\_}reviso.pdf:pdf},
issn = {01674943},
journal = {Archives of Gerontology and Geriatrics},
keywords = {Cognitive functions,Cognitive intervention,Mild cognitive impairment,Physical activity,Systemic review},
number = {1},
pages = {175--180},
title = {{Non-pharmacological interventions on cognitive functions in older people with mild cognitive impairment (MCI)}},
volume = {54},
year = {2012}
}
@article{Tu2010,
abstract = {Papaverine, 1-(3,4-dimethoxybenzyl)-6,7-dimethoxyisoquinoline, a specific inhibitor of phosphodiesterase (PDE) 10A with IC50 values of 36 nM for PDE10A, 1,300 nM for PDE3A and 320 nM for PDE4D, has served as a useful pharmaceutical tool to study the physiological role of PDE10A. Here, we report the radiosynthesis of [11C]papaverine and the in vitro and in vivo evaluation of [11C]papaverine as a potential positron emission tomography (PET) radiotracer for imaging PDE10A in the central nervous system (CNS). The radiosynthesis of papaverine with 11C was achieved by O-methylation of the corresponding des-methyl precursor with [11C]methyl iodide. [11C]papaverine was obtained with {\~{}}70{\%} radiochemical yield and a specific activity {\textgreater}10 Ci/Μmol. In vitro autoradiography studies of rat and monkey brain sections revealed selective binding of [11C]papaverine to PDE10A enriched regions: the striatum of rat brain and the caudate and putamen of rhesus monkey brain. The biodistribution of [11C]papaverine in rats at 5 min demonstrated an initially higher accumulation in striatum than in other brain regions, however the washout was rapid. MicroPET imaging studies in rhesus macaques similarly displayed initial specific uptake in the striatum with very rapid clearance of [11C]papaverine from brain. Our initial evaluation suggests that despite papaverine's utility for in vitro studies and as a pharmaceutical tool, [11C]papaverine is not an ideal radioligand for clinical imaging of PDE10A in the CNS. Analogs of papaverine having a higher potency for inhibiting PDE10A and improved pharmacokinetic properties will be necessary for imaging this enzyme with PET. {\textcopyright} 2010 Elsevier Inc.},
author = {Tu, Zhude and Xu, Jinbin and Jones, Lynne A. and Li, Shihong and Mach, Robert H.},
doi = {10.1016/j.nucmedbio.2009.12.012},
file = {:C$\backslash$:/Users/SHIGURE/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tu et al. - 2010 - Carbon-11 labeled papaverine as a PET tracer for imaging PDE10A radiosynthesis, in vitro and in vivo evaluation.pdf:pdf},
issn = {09698051},
journal = {Nuclear Medicine and Biology},
keywords = {C-11,CNS,PDE10A,PET,Papaverine,Phosphodiesterase 10A,Radiotracer},
number = {4},
pages = {509--516},
pmid = {20447563},
publisher = {Elsevier Inc.},
title = {{Carbon-11 labeled papaverine as a PET tracer for imaging PDE10A: radiosynthesis, in vitro and in vivo evaluation}},
