file = {:C$\backslash$:/Users/SHIGURE/Box Sync/논문/Intake-of-melatonin-increases-tryptophan-hydroxylase-type{\_}2014{\_}Experimental-.pdf:pdf},
issn = {05315565},
journal = {Experimental Gerontology},
keywords = {5-HT,Ageing,Melatonin,Pineal gland,Tryptophan hydroxylase type 1},
number = {1},
pages = {1--4},
publisher = {Elsevier Inc.},
title = {{Intake of melatonin increases tryptophan hydroxylase type 1 activity in aged rats: Preliminary study}},
