isbn = {1168201594},
issn = {19317565},
journal = {Brain Imaging and Behavior},
keywords = {Alzheimer's disease (AD),Graph theory,Machine learning,Mild cognitive impairment (MCI),Resting-state functional magnetic resonance imagin,Support vector machine (SVM)},
