@article{Su2015,
abstract = {Amyloid imaging is a valuable tool for research and diagnosis in dementing disorders. As positron emission tomography (PET) scanners have limited spatial resolution, measured signals are distorted by partial volume effects. Various techniques have been proposed for correcting partial volume effects, but there is no consensus as to whether these techniques are necessary in amyloid imaging, and, if so, how they should be implemented. We evaluated a two-component partial volume correction technique and a regional spread function technique using both simulated and human Pittsburgh compound B (PiB) PET imaging data. Both correction techniques compensated for partial volume effects and yielded improved detection of subtle changes in PiB retention. However, the regional spread function technique was more accurate in application to simulated data. Because PiB retention estimates depend on the correction technique, standardization is necessary to compare results across groups. Partial volume correction has sometimes been avoided because it increases the sensitivity to inaccuracy in image registration and segmentation. However, our results indicate that appropriate PVC may enhance our ability to detect changes in amyloid deposition.},
author = {Su, Yi and Blazey, Tyler M. and Snyder, Abraham Z. and Raichle, Marcus E. and Marcus, Daniel S. and Ances, Beau M. and Bateman, Randall J. and Cairns, Nigel J. and Aldea, Patricia and Cash, Lisa and Christensen, Jon J. and Friedrichsen, Karl and Hornbeck, Russ C. and Farrar, Angela M. and Owen, Christopher J. and Mayeux, Richard and Brickman, Adam M. and Klunk, William and Price, Julie C. and Thompson, Paul M. and Ghetti, Bernadino and Saykin, Andrew J. and Sperling, Reisa A. and Johnson, Keith A. and Schofield, Peter R. and Buckles, Virginia and Morris, John C. and Benzinger, Tammie L.S.},
doi = {10.1016/j.neuroimage.2014.11.058},
file = {:C$\backslash$:/Users/SHIGURE/Box Sync/논문/Partial-volume-correction-in-quantitative-amyloid-imaging{\_}2015{\_}NeuroImage.pdf:pdf},
issn = {10959572},
journal = {NeuroImage},
keywords = {Amyloid imaging,PET,Partial volume correction,PiB},
pages = {55--64},
title = {{Partial volume correction in quantitative amyloid imaging}},
volume = {107},
year = {2015}
}
