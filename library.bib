file = {:C$\backslash$:/Users/SHIGURE/Box Sync/논문/1-s2.0-S0197458006000480-main.pdf:pdf},
issn = {01974580},
journal = {Neurobiology of Aging},
keywords = {Alzheimer's disease,Apolipoprotein E,A$\beta$42,Mild cognitive impairment,Phosphorylated Tau,Tau},
number = {4},
pages = {507--514},
title = {{CSF A$\beta$42, Tau and phosphorylated Tau, APOE $\epsilon$4 allele and MCI type in progressive MCI}},
