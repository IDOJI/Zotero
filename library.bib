issn = {01974580},
journal = {Neurobiology of Aging},
keywords = {Amyloid,CSF biomarkers,Converters,FDG,Mild cognitive impairment,PET,PIB},
number = {10},
pages = {1456--1465},
title = {{PET imaging of amyloid deposition in patients with mild cognitive impairment}},
