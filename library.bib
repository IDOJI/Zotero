issn = {01974580},
journal = {Neurobiology of Aging},
keywords = {11C-Nicotine binding,Acetylcholinesterase,Alzheimer's disease,Attention,Cerebrospinal fluid,Cognitive function,Galantamine,Plasma,Positron emission tomography (PET)},
number = {8},
