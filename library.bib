issn = {03010511},
journal = {Biological Psychology},
keywords = {Attention,DLPFC,Emotion,FMRI,HF-rTMS},
number = {3},
pages = {487--495},
publisher = {Elsevier B.V.},
title = {{Neurocognitive effects of HF-rTMS over the dorsolateral prefrontal cortex on the attentional processing of emotional information in healthy women: An event-related fMRI study}},
url = {http://dx.doi.org/10.1016/j.biopsycho.2010.09.015},
