journal = {Neuroscience},
keywords = {Chick,Kynurenic acid,NMDA receptors,Stress,$\alpha$7nACh receptor},
pages = {194--200},
publisher = {IBRO},
title = {{Two receptors are involved in the central functions of kynurenic acid under an acute stress in neonatal chicks}},
