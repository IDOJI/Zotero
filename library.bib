issn = {01664328},
journal = {Behavioural Brain Research},
keywords = {AMPA/Kainate-receptors,Catalepsy sensitization,Consolidation,Excitatory amino acids,Haloperidol,NMDA-receptors},
number = {1},
pages = {194--199},
publisher = {Elsevier B.V.},
title = {{The role of NMDA and AMPA/Kainate receptors in the consolidation of catalepsy sensitization}},
url = {http://dx.doi.org/10.1016/j.bbr.2010.11.055},
